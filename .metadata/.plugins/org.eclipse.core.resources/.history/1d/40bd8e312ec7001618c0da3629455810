	public static String replaceOne (String text, int i, int n, String sub){
		int origLength = text.length();

		String part1,part2,revisedString;

		part1 = text.substring(0,i);
		part2 = text.substring(i+n, origLength); 

		revisedString = part1 + sub + part2;

		return revisedString;
		}
	
	
	
